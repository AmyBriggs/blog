<div class="container container-custom">
  {{#each users}}
  <div>
    <h4><a href="/users/{{this.id}}">{{this.first_name}} {{this.last_name}}</a></h4>
    <img class="user-image circle responsive-img" src="{{this.img_url}}" alt="{{this.first_name}} + {{this.last_name}}" />
    <p>{{this.bio}}</p>
  </div>
  {{/each}}
</div>
